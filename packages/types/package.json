{
  "name": "@internal-linking-analyzer-pro/types",
  "version": "1.0.0",
  "description": "Shared TypeScript types for Internal Linking Analyzer Pro",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./sitemap": {
      "types": "./dist/sitemap.d.ts",
      "import": "./dist/sitemap.mjs",
      "require": "./dist/sitemap.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts src/sitemap.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts src/sitemap.ts --format cjs,esm --dts --watch",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "eslint": "^9.0.0",
    "rimraf": "^5.0.10",
    "tsup": "^8.1.0",
    "typescript": "^5.7.3"
  },
  "files": [
    "dist"
  ]
}
